<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/mobile_navigation"
            app:startDestination="@+id/nav_profile">

    <fragment
            android:id="@+id/nav_reservations"
            android:name="com.uta.utacarrental.view.reservation.ReservationFragment"
            android:label="Reservation"
            tools:layout="@layout/fragment_reservation"/>
    <fragment
            android:id="@+id/nav_search_user"
            android:name="com.uta.utacarrental.view.search_for_user.SearchForUserFragment"
            android:label="Search for User"
            tools:layout="@layout/fragment_search_user"/>
    <fragment
            android:id="@+id/nav_profile"
            android:name="com.uta.utacarrental.view.profile.ProfileFragment"
            android:label="@string/menu_profile"
            tools:layout="@layout/fragment_profile"/>

    <fragment
            android:id="@+id/nav_search_car"
            android:name="com.uta.utacarrental.view.search_for_car.SearchForCarFragment"
